    <profiles>
        <profile>
            <id>enterprise</id>
            <modules>
                <module>war</module>
            </modules>
        </profile>
        <profile>
            <id>fullBuild</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <modules>
                <module>war</module>
                <module>distribution</module>
                <module>docker-alfresco</module>
                <module>docker-alfresco/aws</module>
            </modules>
        </profile>
        <profile>
            <id>all-tas-tests</id>
            <modules>
                <module>tests</module>
            </modules>
        </profile>
        <profile>
            <id>release</id>
            <modules>
                <module>war</module>
                <module>distribution</module>
                <module>docker-alfresco</module>
            </modules>
        </profile>
        <profile>
            <id>pipeline</id>
            <modules>
                <module>war</module>
                <module>distribution</module>
                <module>docker-alfresco</module>
                <module>tests/pipeline-all-amps</module>
            </modules>
        </profile>
    </profiles>

