		<!-- Import Alfresco Content Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
                <import uri="http://www.alfresco.org/model/workflow/1.0" prefix="wf"/>
	</imports>

	<namespaces>
		<namespace uri="http://www.mycompany.com/model/content/1.0" prefix="mywf"/>
	</namespaces>

	<constraints>
		<!-- The following are the lists of restricted values used by Type and Aspect properties -->
	</constraints>

	<types>
		<!-- Definition of new Content Types -->
	</types>

	<aspects>
		<!-- Definition of new Content Aspect: Image Classification -->
	</aspects>

</model>
